{
	"priority": 1,
	"description": [
		"Get a cell value from a CSV file by row and column index as a localized string."
	],
	"params": [
		{
			"name": "path",
			"types": ["string"],
			"description": ["Path to the file."]
		},
		{
			"name": "row",
			"types": ["int"],
			"description": ["Index of the row."]
		},
		{
			"name": "column",
			"types": ["int"],
			"description": ["Index of the column."]
		}
	],
	"return": {
		"types": ["localized string"],
		"description": ["Value from the resulting cell. Empty istring if the cell does not exist."]
	}
}
