{
	"usage": "weaponInventoryType(<weapon>)",
	"description": [
		"Returns the way this weapon is stored, such as \"altmode\", \"item\", \"offhand\", or \"primary\"."
	],
	"params": [
		{
			"name": "weapon",
			"types": ["string"],
			"description": [
				"The weapon name."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["string"]
	},
	"example": [
		"if (weaponInventoryType(\"rpg_mp\") == \"item\") {\n  // Code\n}"
	]
}
