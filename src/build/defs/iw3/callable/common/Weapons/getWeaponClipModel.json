{
	"usage": "getWeaponClipModel(<weapon>)",
	"description": [
		"Returns the name of the weapon clip/magazine model used for the given weapon."
	],
	"params": [
		{
			"name": "weapon",
			"type": "string",
			"description": [
				"The weapon name."
			],
			"optional": false
		}
	],
	"return": {
		"type": "string"
	},
	"example": [
		"model = getWeaponClipModel(\"m16_basic\");"
	]
}
