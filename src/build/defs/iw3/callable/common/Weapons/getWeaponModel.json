{
	"usage": "getWeaponModel(<weapon>, <variant>)",
	"description": [
		"Returns the name of the weapon model used for the given weapon."
	],
	"params": [
		{
			"name": "weapon",
			"types": ["string"],
			"description": [
				"The weapon name."
			],
			"optional": false
		},
		{
			"name": "variant",
			"types": ["int"],
			"description": [
				"Model variant (0-15)."
			],
			"optional": true
		}
	],
	"return": {
		"types": ["string"]
	},
	"example": [
		"model = getWeaponModel(\"mosin_nagant_sniper\", 0);"
	]
}
