{
	"usage": "weaponMaxAmmo(<weapon>)",
	"description": [
		"Returns the max amount of ammo this weapon is meant to hold. Stockpile only, not the clip."
	],
	"params": [
		{
			"name": "weapon",
			"types": ["string"],
			"description": [
				"The weapon name."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["int"]
	},
	"example": [
		"max = weaponMaxAmmo(\"mp5\");"
	]
}
