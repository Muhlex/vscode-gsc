{
	"usage": "animHasNotetrack(<animation>, <note track>)",
	"description": [
		"Queries the given animation for a note track."
	],
	"params": [
		{
			"name": "animation",
			"types": ["animation"],
			"description": [
				"Reference to an animation."
			],
			"optional": false
		},
		{
			"name": "note track",
			"types": ["string"],
			"description": [
				"A constant string with the name of the note track."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["bool"]
	},
	"example": [
		"if (isDefined(facialanim) && animHasNotetrack(facialanim, \"dialogue\"))"
	]
}
