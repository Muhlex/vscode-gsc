{
	"usage": "getAnimAssetType(<animation>)",
	"description": [
		"Get the animation asset type (delta/relative/additive etc defined in xanim.gdf)."
	],
	"params": [
		{
			"name": "animation",
			"types": ["animation"],
			"description": [
				"Reference to an animation."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["string"]
	},
	"example": [
		"getAnimAssetType(%walk_and_run_loops)"
	]
}
