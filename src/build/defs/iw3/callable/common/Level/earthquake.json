{
	"usage": "earthquake(<scale>, <duration>, <source>, <radius>)",
	"description": [
		"Create an earthquake at the given point."
	],
	"params": [
		{
			"name": "scale",
			"type": "float",
			"description": [
				"The scale of the quake."
			],
			"optional": false
		},
		{
			"name": "duration",
			"type": "float",
			"description": [
				"Duration in seconds"
			],
			"optional": false
		},
		{
			"name": "source",
			"type": "Vector3",
			"description": [
				"The earthquake origin."
			],
			"optional": false
		},
		{
			"name": "radius",
			"type": "int",
			"description": [
				"The earthquake radius of effect."
			],
			"optional": false
		}
	],
	"example": [
		"earthquake(0.3, 3, self.origin, 850);"
	]
}