{
	"usage": "<player> setClientDvars(<dvar>, <value>, <dvar>, <value>...)",
	"description": [
		"Set the values of a list of dvars which are specific to each client."
	],
	"params": [
		{
			"name": "dvar",
			"type": "string",
			"description": [
				"The dvar name."
			],
			"optional": false
		},
		{
			"name": "value",
			"type": "string",
			"description": [
				"The dvar value."
			],
			"optional": false
		}
	],
	"example": [
		"self setClientDvars(\"cg_drawhud\", \"0\", \"cg_draw2d\", \"0\");"
	]
}