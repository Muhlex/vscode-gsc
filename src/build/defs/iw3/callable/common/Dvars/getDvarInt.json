{
	"usage": "getDvarInt(<dvar>)",
	"description": [
		"Gets the value of a dvar, as an integer."
	],
	"params": [
		{
			"name": "dvar",
			"type": "string",
			"description": [
				"The dvar name."
			],
			"optional": false
		}
	],
	"return": {
		"type": "int"
	},
	"example": [
		"level.fogtype = getDvarInt(\"scr_fog_type\");"
	]
}
