{
	"usage": "<player> getWeaponSlotWeapon(<weapon slot>)",
	"description": [
		"Gets the name of the weapon in the given weapon slot."
	],
	"params": [
		{
			"name": "weapon slot",
			"type": "string",
			"description": [
				"Valid weaponslots are \"primary\" and \"primaryb\"."
			],
			"optional": false
		}
	],
	"return": {
		"type": "string"
	},
	"example": [
		"playerWeapon[0] = level.player getWeaponSlotWeapon(\"primary\");"
	]
}
