{
	"usage": "<player> setWeaponSlotWeapon(<weapon slot>, <weapon>)",
	"description": [
		"Sets the weapon in the given slot."
	],
	"params": [
		{
			"name": "weapon slot",
			"types": ["string"],
			"description": [
				"Valid weaponslots are \"primary\" and \"primaryb\"."
			],
			"optional": false
		},
		{
			"name": "weapon",
			"types": ["string"],
			"description": [
				"The name of the weapon to use in this slot."
			],
			"optional": false
		}
	],
	"example": [
		"level.player setWeaponSlotWeapon(\"primary\", \"m16_sp\");"
	]
}
