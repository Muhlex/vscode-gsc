{
	"usage": "<player> getWeaponSlotClipAmmo(<weapon slot>)",
	"description": [
		"Gets the ammunition in the clip for the weapon in the given slot."
	],
	"params": [
		{
			"name": "weapon slot",
			"types": ["string"],
			"description": [
				"Valid weaponslots are \"primary\" and \"primaryb\"."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["int"]
	},
	"example": [
		"ammo = level.player getWeaponSlotClipAmmo(\"primary\");"
	]
}
