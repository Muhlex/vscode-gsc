{
	"usage": "<player> setWeaponSlotAmmo(<weapon slot>, <ammo count>)",
	"description": [
		"Sets the ammunition for the weapon in the given slot."
	],
	"params": [
		{
			"name": "weapon slot",
			"type": "string",
			"description": [
				"Valid weaponslots are \"primary\" and \"primaryb\"."
			],
			"optional": false
		},
		{
			"name": "ammo count",
			"type": "int",
			"description": [
				"The amount of ammunition."
			],
			"optional": false
		}
	],
	"example": [
		"level.player setWeaponSlotAmmo(\"primary\", 125);"
	]
}