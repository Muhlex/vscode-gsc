{
	"usage": "<player> setChannelVolumes(<priority>, <shock name>, <fade time>)",
	"description": [
		"Set the channel volumes for the player (a way of fading volumes by type)."
	],
	"params": [
		{
			"name": "priority",
			"types": ["string"],
			"description": [
				"Valid priorities are \"snd_channelvolprio_holdbreath\", \"snd_channelvolprio_pain\", or \"snd_channelvolprio_shellshock\"."
			],
			"optional": false
		},
		{
			"name": "shock name",
			"types": ["string"],
			"description": [
				"The name of the .shock file w/ the channel volumes values to use."
			],
			"optional": false
		},
		{
			"name": "fade time",
			"types": ["float"],
			"description": [
				"Fade time in seconds."
			],
			"optional": true
		}
	],
	"example": [
		"level.player setChannelVolumes(\"snd_channelvolprio_pain\", \"pain\", 3);"
	]
}
