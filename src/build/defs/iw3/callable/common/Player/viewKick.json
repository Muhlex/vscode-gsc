{
	"usage": "<player> viewKick(<force>, <source>)",
	"description": [
		"Damage the player, and throw the screen around."
	],
	"params": [
		{
			"name": "force",
			"types": ["int"],
			"description": [
				"The force of the kick, from 0 to 127."
			],
			"optional": false
		},
		{
			"name": "source",
			"types": ["Vector3"],
			"description": [
				"The source of the kick."
			],
			"optional": false
		}
	],
	"example": [
		"level.player viewKick(127, level.player.origin);"
	]
}
