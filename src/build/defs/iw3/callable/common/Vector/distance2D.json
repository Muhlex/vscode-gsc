{
	"usage": "distance2D(<point A>, <point B>)",
	"description": [
		"Returns the distance between two points, ignores height difference."
	],
	"params": [
		{
			"name": "point A",
			"type": "Vector3",
			"description": [
				"The first point."
			]
		},
		{
			"name": "point B",
			"type": "Vector3",
			"description": [
				"The second point."
			]
		}
	],
	"return": {
		"type": "float"
	},
	"example": [
		"dist = distance2D(org, ai[i].origin);"
	]
}
