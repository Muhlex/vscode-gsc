{
	"usage": "<hud> setTimer(<time>)",
	"description": [
		"Set a timer HUD element to count down."
	],
	"params": [
		{
			"name": "time",
			"type": "int",
			"description": [
				"The new timer time."
			],
			"optional": false
		}
	],
	"example": [
		"level.reinforcementHud setTimer(level.counter);"
	]
}