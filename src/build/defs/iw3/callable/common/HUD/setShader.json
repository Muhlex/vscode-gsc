{
	"usage": "<hud> setShader(<material>, <width>, <height>)",
	"description": [
		"Set the material for this HUD element."
	],
	"params": [
		{
			"name": "material",
			"types": ["string"],
			"description": [
				"The name of the material to set."
			],
			"optional": false
		},
		{
			"name": "width",
			"types": ["int"],
			"description": [
				"The width of the material."
			],
			"optional": true
		},
		{
			"name": "height",
			"types": ["int"],
			"description": [
				"The height of the material."
			],
			"optional": true
		}
	],
	"example": [
		"waypoint setShader(\"playbook_objective_stop\", 15, 15);"
	]
}
