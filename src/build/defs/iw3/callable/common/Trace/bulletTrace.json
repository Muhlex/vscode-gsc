{
	"usage": "bulletTrace(<start>, <end>, <hit characters>, <ignore entity>)",
	"description": [
		"Allows script to do a point trace with MASK_SHOT. Returns hit position, hit entity, hit surface normal."
	],
	"params": [
		{
			"name": "start",
			"types": ["Vector3"],
			"description": [
				"The bullet start point."
			],
			"optional": false
		},
		{
			"name": "end",
			"types": ["Vector3"],
			"description": [
				"The bullet end point."
			],
			"optional": false
		},
		{
			"name": "hit characters",
			"types": ["bool"],
			"description": [
				"When set to true, this will trace for character hits."
			],
			"optional": false
		},
		{
			"name": "ignore entity",
			"types": ["Entity"],
			"description": [
				"An entity to ignore."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["Array"]
	},
	"example": [
		"trace = bulletTrace(magicBulletOrigin.origin, eyePos, true, undefined);"
	]
}
