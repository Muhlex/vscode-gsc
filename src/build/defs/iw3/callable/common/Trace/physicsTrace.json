{
	"usage": "physicsTrace(<start>, <end>, <hit characters>, <ignore entity>)",
	"description": [
		"Physics trace, ignoring characters. Returns the end position vector."
	],
	"params": [
		{
			"name": "start",
			"types": ["Vector3"],
			"description": [
				"The start point."
			],
			"optional": false
		},
		{
			"name": "end",
			"types": ["Vector3"],
			"description": [
				"The end point."
			],
			"optional": false
		},
		{
			"name": "hit characters",
			"types": ["bool"],
			"description": [
				"Ignore characters in the physics trace."
			],
			"optional": true
		},
		{
			"name": "ignore entity",
			"types": ["entity"],
			"description": [
				"Ignore a specified entity in the physics trace."
			],
			"optional": true
		}
	],
	"return": {
		"types": ["Vector3"]
	},
	"example": [
		"endPos = physicsTrace(start, end, true, ignoredEntity);"
	]
}
