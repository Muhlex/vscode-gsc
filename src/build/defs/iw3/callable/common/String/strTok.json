{
	"usage": "strTok(<string>, <delim>)",
	"description": [
		"Tokenizes string by the delimiters delim. Returns the array of string tokens."
	],
	"params": [
		{
			"name": "string",
			"types": ["string"],
			"description": [
				"String to split into tokens."
			],
			"optional": false
		},
		{
			"name": "delim",
			"types": ["string"],
			"description": [
				"Delimiter to split string by."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["Array"]
	},
	"example": [
		"info = strTok(playerInfo, \",\");"
	]
}
