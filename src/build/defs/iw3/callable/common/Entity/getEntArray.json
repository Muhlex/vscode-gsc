{
	"usage": "getEntArray(<name>, <key>)",
	"description": [
		"Gets an array of entites that have the given KVP."
	],
	"params": [
		{
			"name": "name",
			"types": ["string"],
			"description": [
				"Name to search for."
			],
			"optional": false
		},
		{
			"name": "key",
			"types": ["string"],
			"description": [
				"Key that name goes with."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["Array"]
	},
	"example": [
		"aeExplosions = getEntArray(strExplosion, \"targetname\");"
	]
}
