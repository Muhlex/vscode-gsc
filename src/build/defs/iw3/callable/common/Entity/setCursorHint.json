{
	"usage": "<entity> setCursorHint(<hint>)",
	"description": [
		"Sets a visible cursor near to an objective."
	],
	"params": [
		{
			"name": "hint",
			"type": "string",
			"description": [
				"One of the following strings: \"HINT_NOICON\", \"HINT_ACTIVATE\", \"HINT_HEALTH\", \"HINT_FRIENDLY\"."
			],
			"optional": false
		}
	],
	"example": [
		"trigger setCursorHint(\"HINT_NOICON\");"
	]
}