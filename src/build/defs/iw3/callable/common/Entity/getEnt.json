{
	"usage": "getEnt(<name>, <key>)",
	"description": [
		"Looks up an entity by a given KVP."
	],
	"params": [
		{
			"name": "name",
			"types": ["string"],
			"description": [
				"Name to search for."
			],
			"optional": false
		},
		{
			"name": "key",
			"types": ["string"],
			"description": [
				"Key that name goes with."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["Entity"]
	},
	"example": [
		"spawner = getEnt(\"doorguy1\", \"targetname\");"
	]
}
