{
	"usage": "<entity> setNormalHealth(<health>)",
	"description": [
		"Sets the normal health of this entity."
	],
	"params": [
		{
			"name": "health",
			"type": "int",
			"description": [
				"The new normal health."
			],
			"optional": false
		}
	],
	"example": [
		"self setNormalHealth((self.maxHealth - self.damageTaken) / self.maxHealth);"
	]
}