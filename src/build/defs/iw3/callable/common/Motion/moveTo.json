{
	"usage": "<entity> moveTo(<point>, <time>, <acceleration time>, <deceleration time>)",
	"description": [
		"Move this entity to the given point."
	],
	"params": [
		{
			"name": "point",
			"types": ["Vector3"],
			"description": [
				"The point to move the entity to."
			],
			"optional": false
		},
		{
			"name": "time",
			"types": ["float"],
			"description": [
				"The time to move the entity in seconds."
			],
			"optional": false
		},
		{
			"name": "acceleration time",
			"types": ["float"],
			"description": [
				"The time spent accelerating."
			],
			"optional": true
		},
		{
			"name": "deceleration time",
			"types": ["float"],
			"description": [
				"The time spent decelerating."
			],
			"optional": true
		}
	],
	"example": [
		"dummy moveTo(destOrg, .5, .05, .05);"
	]
}
