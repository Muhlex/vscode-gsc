{
	"usage": "<entity> rotatePitch(<pitch angle>, <time>, <acceleration time>, <deceleration time>)",
	"description": [
		"Rotate this entity to the given pitch relative to its starting angles."
	],
	"params": [
		{
			"name": "pitch angle",
			"type": "float",
			"description": [
				"The amount of degrees to rotate the entity by."
			],
			"optional": false
		},
		{
			"name": "time",
			"type": "float",
			"description": [
				"The time to move the entity in seconds."
			],
			"optional": false
		},
		{
			"name": "acceleration time",
			"type": "float",
			"description": [
				"The time spent accelerating."
			],
			"optional": true
		},
		{
			"name": "deceleration time",
			"type": "float",
			"description": [
				"The time spent decelerating."
			],
			"optional": true
		}
	],
	"example": [
		"treeOrg rotatePitch(-5, 0.26, 0.15, 0.1);"
	]
}