{
	"usage": "fGetArg(<file>, <arg>)",
	"description": [
		"Get a specific argument number from the current line."
	],
	"params": [
		{
			"name": "file",
			"types": ["int"],
			"description": [
				"File handle returned by openFile."
			],
			"optional": false
		},
		{
			"name": "arg",
			"types": ["int"],
			"description": [
				"Argument number."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["string"]
	},
	"example": [
		"data.id = fGetArg(file, 1);"
	]
}
