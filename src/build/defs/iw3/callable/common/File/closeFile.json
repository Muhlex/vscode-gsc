{
	"usage": "closeFile(<file>)",
	"description": [
		"Close a script-controlled file. Returns 1 if successful and -1 if unsuccessful."
	],
	"params": [
		{
			"name": "file",
			"types": ["int"],
			"description": [
				"File handle returned by openFile."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["int"]
	},
	"example": [
		"closeFile(file);"
	]
}
