{
	"usage": "objective_icon(<objective number>, <icon>)",
	"description": [
		"Set the compass icon for an objective."
	],
	"params": [
		{
			"name": "objective number",
			"types": ["int"],
			"description": [
				"The number of the objective to alter."
			],
			"optional": false
		},
		{
			"name": "icon",
			"types": ["string"],
			"description": [
				"A compass icon."
			],
			"optional": false
		}
	],
	"example": [
		"objective_icon(0, game[\"radio_none\"]);"
	]
}
