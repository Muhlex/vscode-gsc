{
	"usage": "<entity> showToPlayer(<player>)",
	"description": ["Show the entity to a given client."],
	"receiver": {
		"name": "entity",
		"types": ["Entity"],
		"description": ["Entity to make visible."]
	},
	"params": [
		{
			"name": "player",
			"types": ["Player"],
			"description": [
				"The player to show the entity to."
			],
			"optional": false
		}
	],
	"example": [
		"self showToPlayer(player);"
	]
}
