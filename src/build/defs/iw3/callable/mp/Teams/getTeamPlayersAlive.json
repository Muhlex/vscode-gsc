{
	"usage": "getTeamPlayersAlive(<team>)",
	"description": [
		"Returns the number of players still alive on a given team."
	],
	"params": [
		{
			"name": "team",
			"type": "string",
			"description": [
				"The team, either \"axis\" or \"allies\"."
			],
			"optional": false
		}
	],
	"return": {
		"type": "int"
	},
	"example": [
		"alive = getTeamPlayersAlive(\"allies\");"
	]
}
