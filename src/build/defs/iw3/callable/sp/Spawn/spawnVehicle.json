{
	"usage": "spawnVehicle(<model>, <targetname>, <vehicletype>, <origin>, <angles>)",
	"description": [
		"Spawns a new turret, eg MG42 or flag gun and returns a reference to it."
	],
	"params": [
		{
			"name": "model",
			"types": ["string"],
			"description": [
				"The name of the model to spawn."
			],
			"optional": false
		},
		{
			"name": "targetname",
			"types": ["string"],
			"description": [
				"Vehicle targetname."
			],
			"optional": false
		},
		{
			"name": "vehicletype",
			"types": ["string"],
			"description": [
				"Vehicle type."
			],
			"optional": false
		},
		{
			"name": "origin",
			"types": ["Vector3"],
			"description": [
				"The position of the vehicle."
			],
			"optional": false
		},
		{
			"name": "angles",
			"types": ["Vector3"],
			"description": [
				"The angle to spawn the vehicle at."
			],
			"optional": false
		}
	],
	"return": {
		"types": ["Entity"]
	},
	"example": [
		"vehicle = spawnVehicle(\"vehicle_blackhawk\", \"vclogger\", \"blackhawk\", (0, 0, 0), (0, 0, 0));"
	]
}
