{
	"usage": "<player> setStance(<stance>)",
	"description": [
		"Sets the stance of the player. Possible values are \"crouch\", \"prone\" and \"stand\"."
	],
	"params": [
		{
			"name": "stance",
			"type": "string",
			"description": [
				"Stance to set (\"crouch\", \"prone\", \"stand\")."
			],
			"optional": false
		}
	],
	"example": [
		"level.player setStance(\"stand\");"
	]
}