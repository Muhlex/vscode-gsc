{
	"usage": "<entity> setSoundBlend(<sound 1>, <sound 2>, <mix>)",
	"description": [
		"Set the sound blend on a blend entity."
	],
	"params": [
		{
			"name": "sound 1",
			"type": "string",
			"description": [
				"The first sound."
			],
			"optional": false
		},
		{
			"name": "sound 2",
			"type": "string",
			"description": [
				"The second sound."
			],
			"optional": false
		},
		{
			"name": "mix",
			"type": "float",
			"description": [
				"The mix between the two sounds, a floating point number between 0 and 1"
			],
			"optional": false
		}
	],
	"example": [
		"blend setBlendSound(\"stalingrad_artillery_rumble_null\", \"stalingrad_artillery_rumble\", 0.4);"
	]
}