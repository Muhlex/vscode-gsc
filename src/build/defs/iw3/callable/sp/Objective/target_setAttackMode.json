{
	"usage": "target_setAttackMode(<target entity>, <mode name>)",
	"description": [
		"Sets how missiles and rockets should approach the target."
	],
	"params": [
		{
			"name": "target entity",
			"type": "Entity",
			"description": [
				"The entity that is the target."
			],
			"optional": false
		},
		{
			"name": "mode name",
			"type": "string",
			"description": [
				"Valid values are \"top\" or \"direct\"."
			],
			"optional": false
		}
	],
	"example": [
		"target_setAttackMode(enemyTank, \"top\");"
	]
}