{
	"usage": "<entity> setAnimTime(<animation>, <time>)",
	"description": [
		"Set the time of an animation."
	],
	"params": [
		{
			"name": "animation",
			"type": "animation",
			"description": [
				"Animation in the tree."
			],
			"optional": false
		},
		{
			"name": "time",
			"type": "float",
			"description": [
				"Normalized time between 0 and 1."
			],
			"optional": false
		}
	],
	"example": [
		"self setAnimTime(%stand_aim, 0.3);"
	]
}