{
	"usage": "getNode(<name>, <key>)",
	"description": [
		"Gets a node with a given name, key pair."
	],
	"params": [
		{
			"name": "name",
			"type": "string",
			"description": [
				"Name to search for."
			],
			"optional": false
		},
		{
			"name": "key",
			"type": "string",
			"description": [
				"Key that name goes with."
			],
			"optional": false
		}
	],
	"return": {
		"type": "Entity"
	},
	"example": [
		"node = getNode(self.target, \"targetname\");"
	]
}
