{
	"usage": "cinematic(<cinematic name>, <exitTime>)",
	"description": [
		"Play the given cinematic."
	],
	"params": [
		{
			"name": "cinematic name",
			"type": "string",
			"description": [
				"The cinematic name."
			],
			"optional": false
		},
		{
			"name": "exitTime",
			"type": "float",
			"description": [
				"Time in seconds of the exit fade."
			],
			"optional": true
		}
	],
	"example": [
		"cinematic(\"cod_end\");"
	]
}