{
	"usage": "<entity> setShadowHint(<priority>)",
	"description": [
		"Sets the shadow hint for the entity."
	],
	"params": [
		{
			"name": "priority",
			"type": "string",
			"description": [
				"\"normal\", \"never\", \"high_priority\", \"low_priority\", \"always\" or \"receiver\""
			],
			"optional": false
		}
	],
	"example": [
		"tankGun setShadowHint(\"normal\");"
	]
}