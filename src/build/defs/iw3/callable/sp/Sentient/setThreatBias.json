{
	"usage": "setThreatBias(<group for>, <group against>, <bias>)",
	"description": [
		"Make first group ignored by second group."
	],
	"params": [
		{
			"name": "group for",
			"types": ["string"],
			"description": [
				"Threat bias group for."
			],
			"optional": false
		},
		{
			"name": "group against",
			"types": ["string"],
			"description": [
				"Threat bias group against."
			],
			"optional": false
		},
		{
			"name": "bias",
			"types": ["int"],
			"description": [
				"Threat bias value."
			],
			"optional": false
		}
	],
	"example": [
		"setThreatBias(\"axis\", \"flanking_team\", 120);"
	]
}
