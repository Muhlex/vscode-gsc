{
	"usage": "vectorAdd(<vector>, <x>, <y>, <z>)",
	"description": ["Add values to a vector."],
	"params": [
		{
			"name": "vector",
			"type": "Vector3",
			"description": ["Vector to add values to."],
			"optional": false
		},
		{
			"name": "x",
			"type": "float",
			"description": ["Value to add to the vector's X value."],
			"optional": false
		},
		{
			"name": "y",
			"type": "float",
			"description": ["Value to add to the vector's Y value."],
			"optional": false
		},
		{
			"name": "z",
			"type": "float",
			"description": ["Value to add to the vector's Z value."],
			"optional": false
		}
	],
	"return": {
		"type": "Vector3"
	},
	"example": ["vec = vectorAdd(vec, 1, 0, 2);"]
}
