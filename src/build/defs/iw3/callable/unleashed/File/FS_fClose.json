{
	"usage": "FS_fClose(<file handle>)",
	"description": ["Closes a file opened by FS_fOpen. Opened files need to be closed after usage."],
	"params": [
		{
			"name": "file handle",
			"types": ["int"],
			"description": ["File handle returned by FS_fOpen."],
			"optional": false
		}
	],
	"example": ["FS_fClose(file);"]
}
