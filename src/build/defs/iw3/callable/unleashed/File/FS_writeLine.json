{
	"usage": "FS_writeLine(<file handle>, <data>)",
	"description": ["Writes/appends (depending on the mode used with FS_fOpen) a line to an opened file."],
	"params": [
		{
			"name": "file handle",
			"types": ["int"],
			"description": ["File handle returned by FS_fOpen."],
			"optional": false
		},
		{
			"name": "data",
			"types": ["string"],
			"description": ["Data to be written."],
			"optional": false
		}
	],
	"return": {
		"types": ["string"]
	},
	"example": ["FS_writeLine(file, \"connect;\" + self getGuid());"]
}
